{
    "env": {
      "es6": true,
      "browser": true,
      "node": false
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn"
    },
    "plugins": [
      "react"
    ],
    "extends": ["eslint:recommended"]
  }
  